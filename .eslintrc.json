{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["import"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "unknown",
          "sibling",
          "index",
          "object",
          "type"
        ]
      }
    ]
  }
}
